{
  "name": "backbone-modal",
  "version": "1.1.0",
  "description": "Create clean modal windows with sleek transitions and a simple API.",
  "main": "src/index.js",
  "files": [
    "./src"
  ],
  "scripts": {
    "start": "parcel demo/index.html --out-dir demo-build",
    "build": "parcel build demo/index.html --out-dir demo-build",
    "gh-pages-build": "parcel build demo/index.html --out-dir demo-build --public-url ./",
    "gh-pages-deploy": "node node_modules/push-dir/bin/push-dir.js --dir=demo-build --branch=gh-pages --cleanup",
    "gh-pages": "parcel build demo/index.html --out-dir demo-build --public-url ./ && node node_modules/push-dir/bin/push-dir.js --dir=demo-build --branch=gh-pages --cleanup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjantzer/backbone-modal"
  },
  "keywords": [
    "modal",
    "popup",
    "alert",
    "confirmation",
    "prompt"
  ],
  "author": "Kevin Jantzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kjantzer/backbone-modal/issues"
  },
  "homepage": "https://github.com/kjantzer/backbone-modal",
  "devDependencies": {
    "parcel-bundler": "^1.9.6",
    "push-dir": "^0.4.1"
  },
  "dependencies": {
    "backbone": ">=0.9.10",
    "basic-buttons": "^1.2.0",
    "jquery": "^3.3.1",
    "settings": "^0.1.1",
    "spin.js": "2.3.2",
    "underscore": "^1.8.3"
  }
}
